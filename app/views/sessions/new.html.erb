<h1>Log in</h1>
<%= form_with(scope: :session, url: sessions_path, local: true) do |f| %>
  <div class="box">
  <%= f.label :email %>
  <%= f.email_field :email %>
</div>
<div class="box">
  <%= f.label :password %>
  <%= f.password_field :password %>
</div>
<div class="box">
  <%= f.submit "Log in",class:"btn btn-info" %>
</div>
<% end %>
